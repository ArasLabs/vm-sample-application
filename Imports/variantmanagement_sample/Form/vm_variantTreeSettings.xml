    <css>@import  '../Modules/aras.innovator.VariantManagementSample/styles/cfgTree.css';

#groupsGrid {
	width: 320px;
	height: 320px;
	overflow: hidden;
	border: 1px solid #606060;
	padding-top: 5px;
	transition: padding-top 0.1s ease, height 0.1s ease;
}

#groupsGrid:before {
	content: '';
	display: none;
	background-image: url('../images/checkbox-checked.svg'), url('../images/checkbox-disabled.svg');
}

.moveUpButton, .moveDownButton, .searchButton {
	background-color: #FFFFFF;
	background-size: 14px 20px;
	background-position: 2px 2px;
	background-repeat: no-repeat;
	border: 1px solid #A0A0A0;
	width: 20px !important;
	padding: 0px !important;
	min-width: 20px !important;
	height: 26px !important;
}

.searchInput {
	display: none;
	height: 20px;
	border: 1px dashed #C0C0C0;
	padding: 1px 3px;
	font-size: 12px;
}

.NameElementMatch {
	background-color: #F7EAAD;
}

.searchButton {
	background-image: url('../images/Search.svg');
	background-size: 14px 16px;
	background-position: 2px 3px;
	height: 24px !important;
}

.moveUpButton {
	background-image: url('../images/PageUp.svg');
}

.moveDownButton {
	background-image: url('../images/PageDown.svg');
}

.moveUpButton:hover, .moveDownButton:hover {
	border: 1px solid #808080;
	background-color: #DDE7F5;
}

.arasCFGTree.dojoxGrid .ActionButton.checkboxActionButton {
	width: 16px;
	height: 16px;
	background-image: url('../images/checkbox-checked.svg');
	background-size: 16px 16px;
	background-position: 0px 0px;
	background-repeat: no-repeat;
	border: none;
}

.arasCFGTree.dojoxGrid .ActionButton.checkboxActionButton.Unchecked {
	visibility: visible;
	background-image: url('../images/checkbox-disabled.svg');
}

.lineSeparator {
	border-bottom: 1px solid #A0A0A0;
}

.applySettingsButton {
	min-width: auto;
	width: 94px;
}</css>
