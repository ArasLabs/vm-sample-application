<?xml version="1.0" encoding="utf-8"?>
<package>
  <info>
    <name>CS_Sample</name>
    <label>Aras Configurator Services – Sample</label>
    <version>12.0.7291</version>
    <version_label>12.0.0.1</version_label>
    <type>aia</type>
    <owner>Aras Corporation</owner>
    <description>This sample application provides examples to show how Configurator Services APIs and UI Controls can be used to build custom applications for variant management.</description>
    <copyright>©2019 Aras Corporation</copyright>
    <package_icon>package.ico</package_icon>
    <more_information_url>http://www.aras.com/solutions/variant-management</more_information_url>
    <tool_minimal_version_required>1.1</tool_minimal_version_required>
  </info>
  <installation>
    <module id="IS" label="@{Pkg.IS.Label}" description="@{Pkg.IS.Desc}" required_properties="Innovator.Dir">
      <property name="IncludeNamespaceConfigPath" value="${Innovator.Dir}\Client\javascript\includeNamespaceConfig.xml" />
      <property name="variantsTreeCssXPath" value="/IncludeNamespaces/CSSNamespace/class[@name='variantsTree.css']/dependencies" />
      <property name="variantsTreeJavascriptXPath" value="/IncludeNamespaces/JavaScriptNamespace" />
      <property name="variantsTreeCssClass" value="&#xD;&#xA;		&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Styles\LabeledRangeSlider.css&quot; /&gt;&#xD;&#xA;		&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Styles\variantsTree.css&quot; /&gt;&#xD;&#xA;			" />
      <xmlpeek file="${IncludeNamespaceConfigPath}" xpath="${variantsTreeCssXPath}" property="oldVariantsTreeCssClass" />
      <property name="newVariantsTreeCss" value="${oldVariantsTreeCssClass}${variantsTreeCssClass}" />
      <xmlpoke file="${IncludeNamespaceConfigPath}" xpath="${variantsTreeCssXPath}" value="${newVariantsTreeCss}" failonerror="true" />
      <property name="variantTreeJavascriptClasses" value="&#xD;&#xA;	&lt;file name=&quot;classList&quot; src=&quot;..\Modules\polyfills\classList.js&quot; browser_code_condition=&quot;^ie$&quot;/&gt;&#xD;&#xA;	&lt;file name=&quot;gunzip.min.js&quot; src=&quot;..\Modules\aras.innovator.Configurator\Scripts\3rdPartyLibs\gunzip.min.js&quot;/&gt;&#xD;&#xA;	&lt;class name=&quot;publishVariantTree&quot;&gt;&#xD;&#xA;		&lt;dependencies&gt;&#xD;&#xA;			&lt;file src=&quot;..\vendors\d3.min.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Printing\Scripts\jspdf.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\3rdPartyLibs\html2canvas.min.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\3rdPartyLibs\html2canvas.svg.min.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;dojo\dojo.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;dojo\_base\declare.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\javascript\dojo\commonForVariantTreePublishing.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;dojo\html.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\VariantsTree\Publishing\d3.mock.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;Aras\Client\Controls\Experimental\MenuItem.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;Aras\Client\Controls\Experimental\ContextMenu.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.core.Controls\Scripts\Controls\Common\RenderUtils.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\ScopeRules\ModelEnums.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\ScopeRules\ModelItem.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\ScopeRules\DataStore.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\VariantsTree\LabeledRangeSlider.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.core.Core\Scripts\Classes\Eventable.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.core.Controls\Scripts\Controls\VariantsTree\Layers\VisualizationLayer.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.core.Controls\Scripts\Controls\VariantsTree\Layers\TreeVisualizationLayer.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.core.Controls\Scripts\Controls\VariantsTree\Layers\GroupsVisualizationLayer.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\VariantsTree\VariantsTreeDataBuilder.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.core.Controls\Scripts\Controls\VariantsTree\TooltipControl.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.core.Controls\Scripts\Controls\VariantsTree\MultiLayeredView.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.core.Controls\Scripts\Controls\VariantsTree\VariantsTree.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\polyfills\promises.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\VariantsTree\Publishing\ModulesManager.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Printing\Scripts\Classes\PrintingToPdf.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Printing\Scripts\Classes\DomToSVG.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\VariantsTree\Print\VariantsTreePrint.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\VariantsTree\VariantsTreeApplicationBase.js&quot;/&gt;&#xD;&#xA;			&lt;file src=&quot;..\Modules\aras.innovator.Configurator\Scripts\VariantsTree\VariantsTreeApplicationPublish.js&quot;/&gt;&#xD;&#xA;		&lt;/dependencies&gt;&#xD;&#xA;	&lt;/class&gt;" />
      <xmlpeek file="${IncludeNamespaceConfigPath}" xpath="${variantsTreeJavascriptXPath}" property="oldJavascriptClasses" />
      <property name="newJavascriptClasses" value="${oldJavascriptClasses}${variantTreeJavascriptClasses}" />
      <xmlpoke file="${IncludeNamespaceConfigPath}" xpath="${variantsTreeJavascriptXPath}" value="${newJavascriptClasses}" failonerror="true" />
      <copy todir="${Innovator.Dir}/Client" failonerror="true">
        <fileset basedir="${Package.Dir}/files/Innovator/Client">
          <include name="**" />
        </fileset>
      </copy>
    </module>
    <module id="DB" label="@{Pkg.DB.Label}" description="@{Pkg.DB.Desc}" required_properties="Innovator.URL|Innovator.DB|Innovator.Username|Innovator.Password">
      <dependencies>
        <innovator version="12.0 None" />
        <application exists="true" name="com.aras.innovator.solution.PLM" />
      </dependencies>
      <import dir="${Package.Dir}/Imports/" failonerror="true" />
    </module>
    <module id="DL" label="Sample Data Actions" description="Optional. Imports methods for adding/removing variant sample data." required_properties="Innovator.URL|Innovator.DB|Innovator.Username|Innovator.Password">
      <!-- Import data load methods -->
      <import dir="${Package.Dir}/SampleData/Import/" failonerror="true" />
    </module>
  </installation>
</package>